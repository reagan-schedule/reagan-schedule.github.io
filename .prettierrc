{
	"useTabs": true,
	"singleQuote": true,
	"trailingComma": "es5",
	"printWidth": 80,
	"plugins": ["prettier-plugin-svelte", "prettier-plugin-tailwindcss"],
	"experimentalTernaries": true,
	"experimentalOperatorPosition": "start",
	"objectWrap": "collapse",
	"bracketSameLine": false,
	"svelteAllowShorthand": false,
	"overrides": [
		{
			"files": "*.svelte",
			"options": {
				"parser": "svelte"
			}
		}
	]
}
